#include "qcom-ipq4019-habanero.dtsi"

/ {
	model = "8devices Habanero DVK-C2";
	compatible = "8dev,habanero-dvk-c2", "8dev,habanero", "qcom,ipq4019";

	soc {
		usb2@60f8800 {
			status = "okay";
		};

		usb3@8af8800 {
			status = "okay";
		};

		serial@78b0000 {
			pinctrl-0 = <&serial_1_pins>;
			pinctrl-names = "default";
			status = "okay";
		};

		nand: qpic-nand@79b0000 {
			pinctrl-0 = <&nand_pins>;
			pinctrl-names = "default";
			status = "okay";
		};
	};

	gpio-keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			gpios = <&tlmm 8 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		io37 {
			label = "habanero_dvk:green:io37";
			gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
		};
		io40 {
			label = "habanero_dvk:green:io40";
			gpios = <&tlmm 40 GPIO_ACTIVE_HIGH>;
		};
		// Disabled in this config, due to clash with SPI1_MOSI
		//io46 {
		//	label = "habanero_dvk:green:io46";
		//	gpios = <&tlmm 46 GPIO_ACTIVE_HIGH>;
		//};
		io48 {
			label = "habanero_dvk:green:io48";
			gpios = <&tlmm 48 GPIO_ACTIVE_HIGH>;
		};
	};
};

&sdhci {
	pinctrl-0 = <&sd_pins>;
	pinctrl-names = "default";
	cd-gpios = <&tlmm 22 0x1>;
	vqmmc-supply = <&vqmmc>;
	status = "okay";
};

&blsp1_spi2 {
	pinctrl-0 = <&spi_1_pins>;
	pinctrl-names = "default";
	status = "okay";
	cs-gpios = <&tlmm 45 GPIO_ACTIVE_HIGH>, <&tlmm 49 GPIO_ACTIVE_HIGH>;

	spidev@0 {
		compatible = "spidev";
		spi-max-frequency = <24000000>;
		reg = <0>;
	};
	spidev@1 {
		compatible = "spidev";
		spi-max-frequency = <24000000>;
		reg = <1>;
	};
};

&blsp1_i2c3 {
	pinctrl-0 = <&i2c_0_pins>;
	pinctrl-names = "default";
	status = "ok";
};

&blsp1_i2c4 {
	pinctrl-0 = <&i2c_1_pins>;
	pinctrl-names = "default";
	status = "ok";
};

&pcie0 {
	perst-gpio = <&tlmm 38 GPIO_ACTIVE_LOW>;
	wake-gpio = <&tlmm 50 GPIO_ACTIVE_LOW>;
	status = "okay";
};

&tlmm {
	serial_1_pins: serial1_pinmux {
		pinmux {
			pins = "gpio8", "gpio9", "gpio10", "gpio11";
			function = "blsp_uart1";
			bias-disable;
		};
	};

	spi_1_pins: spi_1_pinmux {
		pinmux {
			function = "blsp_spi1";
			pins = "gpio44", "gpio46", "gpio47";
			drive-strength = <12>;
			bias-disable;
		};
		pinmux_cs {
			function = "gpio";
			pins = "gpio45", "gpio49";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};

	i2c_0_pins: i2c_0_pinmux {
		pinmux {
			function = "blsp_i2c0";
			pins = "gpio20", "gpio21";
		};
		pinconf {
			pins = "gpio20", "gpio21";
			drive-strength = <16>;
			bias-disable;
		};
	};

	i2c_1_pins: i2c_1_pinmux {
		pinmux {
			function = "blsp_i2c1";
			pins = "gpio34", "gpio35";
		};
		pinconf {
			pins = "gpio34", "gpio35";
			drive-strength = <16>;
			bias-disable;
		};
	};

	nand_pins: nand_pins {
		pullups {
			pins =  "gpio52", "gpio53", "gpio58", "gpio59";
			function = "qpic";
			bias-pull-up;
		};

		pulldowns {
			pins = "gpio54", "gpio55", "gpio56", "gpio57",
				"gpio60", "gpio62", "gpio63", "gpio64",
				"gpio65", "gpio66", "gpio67", "gpio68",
				"gpio69";
			function = "qpic";
			bias-pull-down;
		};
	};

	sd_pins: sd_pins {
		pinmux {
			function = "sdio";
			pins = "gpio23", "gpio24", "gpio25", "gpio26",
				"gpio28", "gpio29", "gpio30", "gpio31";
			drive-strength = <10>;
		};
		pinmux_sd_clk {
			function = "sdio";
			pins = "gpio27";
			drive-strength = <16>;
		};
		pinmux_sd7 {
			function = "sdio";
			pins = "gpio32";
			drive-strength = <10>;
			bias-disable;
		};
	};
};

&usb2_hs_phy {
	status = "okay";
};

&usb3_hs_phy {
	status = "okay";
};

&usb3_ss_phy {
	status = "okay";
};
