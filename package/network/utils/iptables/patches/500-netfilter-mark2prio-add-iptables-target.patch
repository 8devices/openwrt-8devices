+#include <stdbool.h>
+#include <stdio.h>
+#include <string.h>
+#include <stdlib.h>
+#include <getopt.h>
+#include <xtables.h>
+#include <linux/version.h>
+
+static struct xtables_target mark2prio_targets[] = {
+	{
+		.name		= "mark2prio",
+		.version	= XTABLES_VERSION,
+		.family		= NFPROTO_IPV4,
+		.size		= 0,
+		.userspacesize	= 0,
+	},
+	{
+		.name		= "mark2prio",
+		.version	= XTABLES_VERSION,
+		.family		= NFPROTO_IPV6,
+		.size		= 0,
+		.userspacesize	= 0,
+	},
+};
+
+void _init(void)
+{
+	xtables_register_targets(mark2prio_targets,
+				ARRAY_SIZE(mark2prio_targets));
+}
