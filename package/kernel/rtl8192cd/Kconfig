	bool "PCIe slot 0 Enable external PA"
	depends on USE_PCIE_SLOT_0 && ((SLOT_0_92C || SLOT_0_88E || SLOT_0_8812 || SLOT_0_8812AR_VN || SLOT_0_8192EE) || ((SLOT_0_8814AE || SLOT_0_8194AE)&&(!SLOT_0_RFE_TYPE_7)&&(!SLOT_0_RFE_TYPE_9)) || (SLOT_0_8822BE && (!SLOT_0_RFE_TYPE_0 && !SLOT_0_RFE_TYPE_8 && !SLOT_0_RFE_TYPE_9 && !SLOT_0_RFE_TYPE_10 && !SLOT_0_RFE_TYPE_13 && !SLOT_0_RFE_TYPE_14)))
	default n

config SLOT_0_EXT_LNA
	bool "PCIe slot 0 Enable external LNA"
	depends on USE_PCIE_SLOT_0 && ((SLOT_0_92C || SLOT_0_88E || SLOT_0_8812 || SLOT_0_8812AR_VN || SLOT_0_8192EE) || ((SLOT_0_8814AE || SLOT_0_8194AE) && (!SLOT_0_RFE_TYPE_7) && (!SLOT_0_RFE_TYPE_9)) || ((SLOT_0_8822BE) && (!SLOT_0_RFE_TYPE_0 && !SLOT_0_RFE_TYPE_8 && !SLOT_0_RFE_TYPE_10 && !SLOT_0_RFE_TYPE_13 && !SLOT_0_RFE_TYPE_14)))
	default n

config SLOT_0_TX_BEAMFORMING
	bool "PCIe slot 0 Enable Tx Beamforming"
	depends on USE_PCIE_SLOT_0  && (SLOT_0_8192EE || SLOT_0_8812 ||SLOT_0_8814AE || SLOT_0_8194AE || SLOT_0_8822BE)
	default y

